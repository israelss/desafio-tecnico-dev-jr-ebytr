{
  "name": "ebytr_api",
  "version": "1.0.0",
  "description": "Server for Ebytr Task Manager",
  "main": "index.js",
  "scripts": {
    "db:deploy": "prisma generate && prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "clean": "rm -rf ./build/* ",
    "build": "tsc",
    "predev": "npm run build && npm run db:migrate:dev",
    "dev": "nodemon --watch src src/server.ts",
    "prestart": "npm run build && npm run db:deploy",
    "start": "node build/server.js",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/israelss/desafio-tecnico-dev-jr-ebytr/api"
  },
  "author": "Israel Sant'Anna @israelss",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "dotenv": "^16.1.4",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "nodemon": "^2.0.16",
    "prisma": "^3.15.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.7.4",
    "vitest": "^0.15.2"
  }
}
